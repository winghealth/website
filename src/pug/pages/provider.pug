extends ../layouts/default.pug

block config

    //- Global Page Configuration
    - var pageTitle = 'Wing Health'


block content

    //- * * * * * * * *
    //- * * Navbar  * *
    //- * * * * * * * *
    include mixins/navbar.pug
    +navbar({
        navbarBg: 'bg-white',
        navbarStyle: 'navbar-light',
        navbarBrandColor: 'text-dark',
        navbarBtnColor: 'btn-primary',
        navbarContainer: 'container'
    })



    include mixins/header.pug
    +header({
        pageHeaderBg: 'bg-white',
        pageHeaderStyle: 'page-header-white',
        svgBorderFill: 'text-dark'
    })

        .page-header-content
            .container
                .row.align-items-center
                    .col-lg-7
                        h1.page-header-title Local opportunity for providers
                        p.page-header-text.md-5 Source new and existing patients, schedule visits and bill for virtual care
                    .col-lg-5
                        img.img-fluid(src='assets/img/svgs/doctor-woman.svg', height="50px").mb-5
                .row.align-items-center
                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    .h1 Sign up, meet patients
                                    .p Connect with individuals in your community seeking your expertise
                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    .h1 Onboard to telehealth
                                    .p Attend one of our information sessions and ask any questions
                    .col-lg-4.mb-5.mb-lg-0
                        .card.h-100
                            .card-body.p-5
                                .text-center
                                    .h1 Bill Medicare, Medicaid and private insurance
                                    .p New regulation expands coverage
